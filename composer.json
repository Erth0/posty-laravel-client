{
    "name": "erth0/posty-laravel-client",
    "description": "Simple yet easy way to store get up and running with a personal blog which uses markdown",
    "license": "MIT",
    "authors": [
        {
            "name": "Eluert Mukja",
            "email": "e.mukja@icloud.com",
            "homepage": "https://mukja.dev"
        }
    ],
    "homepage": "https://github.com/Erth0/posty-laravel-client",
    "keywords": ["Laravel", "Posty"],
    "require": {
        "illuminate/support": "~7|~8",
        "spatie/laravel-sluggable": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Mukja\\Posty\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mukja\\Posty\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mukja\\Posty\\PostyServiceProvider"
            ],
            "aliases": {
                "Posty": "Mukja\\Posty\\Facades\\Posty"
            }
        }
    }
}
